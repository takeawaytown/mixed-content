{
    "name": "takeawaytown/mixed-content",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Scott",
            "email": "codetheorist@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TakeawayTown\\MixedContent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TakeawayTown\\MixedContent\\": "src"
        }
    },
    "require": {
        "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "laravel/framework": "5.2.*|5.3.*|5.4.*|5.5.*",
        "mockery/mockery": "dev-master",
        "orchestra/testbench": "3.4.*@dev",
        "phpunit/phpunit": "5.5.*|~6.0",
        "phpunit/php-code-coverage": "^5.2@dev",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TakeawayTown\\MixedContent\\MixedContentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
